{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "allowJs": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "exclude": [
    "./node_modules"
  ]
}
